{
 "plugins": ["prettier-plugin-tailwindcss"],
 "arrowParens": "always",
 "bracketSameLine": true,
 "bracketSpacing": true,
 "endOfLine": "lf",
 "htmlWhitespaceSensitivity": "strict",
 "jsxSingleQuote": true,
 "printWidth": 120,
 "proseWrap": "always",
 "quoteProps": "as-needed",
 "semi": true,
 "singleQuote": false,
 "tabWidth": 2,
 "trailingComma": "es5",
 "useTabs": false
}
